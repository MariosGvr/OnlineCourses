    "def pper (n,k,y):\n",
    "    if k == y:\n",
    "        return 1\n",
    "    else:\n",
    "        y += 1\n",
    "        return n*pper(n-1,k,y)\n",
    "\n",
    "\n",
    "n= int(input(\"n : \"))\n",
    "k= int(input('k :'))\n",
    "y=0\n",
    "result=str( pper(n,k,y))\n",
    "print(result[-6:])\n"
   ]
